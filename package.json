{
	"name": "synfinder",
	"displayName": "SynFinder",
	"publisher": "tontsa28",
	"description": "Find synonyms for selected word directly in VSCode",
	"icon": "logo/synfinder_logo_128x128.png",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.53.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/tontsa28/synfinder.git"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:synfinder.synfinder"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "synfinder.synfinder",
				"title": "synfinder"
			}
		]
	},
	"keywords": [
		"synonym",
		"syn",
		"synonyms",
		"synfinder",
		"finder",
		"tontsa28",
		"tontsa"
	],
	"author": "tontsa28",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/tontsa28/synfinder/issues"
	},
	"homepage": "https://github.com/tontsa28/synfinder#readme",
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/node-fetch": "^2.5.9",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"node-fetch": "^2.6.1",
		"vscode": "^1.1.37"
	}
}
